                        <div class="pull-left">
                          <p class="profile-title text-bricky">325 eclaireurs</p>
                        </div>
                        <div class="pull-right text-bricky text-success">
                          <i class="fa fa-users fa-lg"></i>
                        </div>
                      </a>
                    </div>
                  </div>
                </div>
                <div class="col-lg-2 col-sm-6 col-md-6 col-xs-12">
                  <div class="panel panel-default mar-no">
                    <div class="panel-body">
                      <a href="JavaScript:void(0);">
                        <div class="pull-left">
                          <p class="profile-title text-bricky">123 cheminot</p>
                        </div>
                        <div class="pull-right text-bricky" style="color: orange">
                          <i class="fa fa-users fa-lg"></i>
                        </div>
                      </a>
                    </div>
                  </div>
                </div>
                <div class="col-lg-2 col-sm-6 col-md-6 col-xs-12">
                  <div class="panel panel-default mar-no">
                    <div class="panel-body">
                      <a href="JavaScript:void(0);">
                        <div class="pull-left">
                          <p class="profile-title text-bricky">152 routiers</p>
                        </div>
                        <div class="pull-right text-bricky" style="color: red">
                          <i class="fa fa-users fa-lg"></i>
                        </div>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!--Page Title-->

          {% block body %}{% endblock %}
