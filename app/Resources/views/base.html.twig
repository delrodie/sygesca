                <a href="#" data-toggle="dropdown" class="dropdown-toggle">
                  <i class="fa fa-bell fa-lg"></i>
                  <span class="badge badge-header badge-danger">5</span>
                </a>
                <!--Notification dropdown menu-->
                <div class="dropdown-menu dropdown-menu-md with-arrow">
                  <div class="pad-all bord-btm">
                    <div class="h4 text-muted text-thin mar-no">
                      Notification
                    </div>
                  </div>
                  <div class="nano scrollable">
                    <div class="nano-content">
                      <ul class="head-list">
                        <!-- Dropdown list-->
                        <li>
                          <a href="#" class="media">
                            <div class="media-left">
                              <span class="icon-wrap icon-circle bg-primary">
                                <i class="fa fa-edit fa-lg"></i>
                              </span>
                            </div>
                            <div class="media-body">
                              <div class="text-nowrap">Abengourou</div>
                              <small class="text-muted">15 nouveaux cotisants</small>
                            </div>
                          </a>
                        </li>
                        <!-- Dropdown list-->
                        <li>
                          <a href="#" class="media">
                            <div class="media-left">
                              <span class="icon-wrap icon-circle bg-success">
                                <i class="fa fa-check-square-o fa-lg"></i>
                              </span>
                            </div>
                            <div class="media-body">
                              <div class="text-nowrap">Nation</div>
                              <small class="text-muted">15 cotisants validés</small>
                            </div>
                          </a>
                        </li>
                        <!-- Dropdown list-->
                        <li>
                          <a href="#" class="media">
                            <div class="media-left">
                              <span class="icon-wrap icon-circle bg-danger">
                                <i class="fa fa-close fa-lg"></i>
                              </span>
                            </div>
                            <div class="media-body">
                              <div class="text-nowrap">Nation</div>
                              <small class="text-muted">15 cotisants non validés</small>
                            </div>
                          </a>
                        </li>
                      </ul>
                    </div>
                  </div>
                  <!--Dropdown footer-->
                  <div class="pad-all bord-top">
                    <a href="#" class="btn-link text-dark box-block">
                      <i class="fa fa-angle-right fa-lg pull-right"></i>Voir toutes les notifications
                    </a>
                  </div>
                </div>
              </li>
              <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
              <!--End notifications dropdown-->
            </ul>
            <ul class="nav navbar-top-links pull-right">
              <!--Fullscreen toogle button-->
              <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
              <li class="hidden-xs" id="toggleFullscreen">
                <a class="fa fa-expand" data-toggle="fullscreen" href="#" role="button">
                  <span class="sr-only">Toggle fullscreen</span>
                </a>
              </li>
              <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
              <!--End Fullscreen toogle button-->
              <!--User dropdown-->
              <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
              <li id="dropdown-user" class="dropdown">
                <a href="#" data-toggle="dropdown" class="dropdown-toggle text-right">
                  <span class="pull-right">
                    <img class="img-circle img-user media-object" src="{{ asset('ressources/img/avatar.png')}}" alt="Profile Picture">
                  </span>
                  <div class="username hidden-xs">{{ 'layout.user'|trans({'%username%': app.user.username}, 'FOSUserBundle')|title }}</div>
                </a>
                <div class="dropdown-menu dropdown-menu-right with-arrow">
                  <!-- User dropdown menu -->
                  <ul class="head-list">
                    <li>
                      <a href="{{ path('fos_user_change_password')}}">
                        <i class="fa fa-cog fa-fw"></i>
                        Changer mot de passe
                      </a>
                    </li>
                    <li>
                      <a href="{{ path('fos_user_security_logout')}}">
                        <i class="fa fa-sign-out fa-fw"></i>
                        Deconnexion
                      </a>
                    </li>
                  </ul>
                </div>
              </li>
              <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
              <!--End user dropdown-->
              <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            </ul>
          </div>
          <!--================================-->
          <!--End Navbar Dropdown-->
        </div>
